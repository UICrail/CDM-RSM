    <!-- https://cdm.ovh/rsm/adapters/geosparql_adapter/geo_ad#hasMetricLength -->

    <owl:DatatypeProperty rdf:about="https://cdm.ovh/rsm/adapters/geosparql_adapter/geo_ad#hasMetricLength">
        <rdfs:subPropertyOf rdf:resource="http://www.opengis.net/ont/geosparql#hasMetricLength"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://cdm.ovh/rsm/localisation/localisation#LinearLocation"/>
