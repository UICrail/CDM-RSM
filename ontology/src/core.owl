    <!-- https://cdm.ovh/rsm/adapters/geosparql_adapter/geosparql_adapter#Feature -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/adapters/geosparql_adapter/geosparql_adapter#Feature"/>
    


    <!-- https://cdm.ovh/rsm/topology/topo#BoundaryElement -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#BoundaryElement">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#LinearElement"/>
        <rdfs:label xml:lang="en">Boundary element</rdfs:label>
        <skos:definition xml:lang="en">Linear element with length = 0. It still has two distinct port, allowing to express connectivity and navigability. To be used at the borders of networks managed by different entities.</skos:definition>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#Connection -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#Connection">
        <rdfs:comment xml:lang="en">Relation between two distinct ports of one (loop case) or two (common case) net  elements. This relation is understood as &quot;adjacency&quot;, i.e. the related ports are expected to be at the same coordinates or with a minimal gap. A rare exception would be the case of transfer bridges, turntables, or the like.</rdfs:comment>
        <rdfs:label xml:lang="en">Connection</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#LinearElement -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#LinearElement">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#NetElement"/>
        <owl:disjointWith rdf:resource="https://cdm.ovh/rsm/topology/topo#NonlinearElement"/>
        <rdfs:label xml:lang="en">Linear element</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#ListedPort -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#ListedPort">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/list#OWLList"/>
        <rdfs:label xml:lang="en">Listed port</rdfs:label>
        <skos:definition xml:lang="en">A port in an ordered list (with successors; no contents needed). Can be used to sort the ports of a net element. See also OWLList ontology.</skos:definition>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#Navigability -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#Navigability">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#NavigabilityRelation"/>
        <owl:disjointWith rdf:resource="https://cdm.ovh/rsm/topology/topo#NonNavigability"/>
        <rdfs:comment xml:lang="en">a (reified) relation between two ports of two distinct net elements.</rdfs:comment>
        <rdfs:label xml:lang="en">Navigability</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#NavigabilityRelation -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#NavigabilityRelation">
        <rdfs:label xml:lang="en">Navigability relation</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#NetElement -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#NetElement">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/adapters/geosparql_adapter/geosparql_adapter#Feature"/>
        <owl:disjointWith rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">Net element</rdfs:label>
        <skos:definition xml:lang="en">Superclass for net elements (network elements)</skos:definition>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#NonNavigability -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#NonNavigability">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#NavigabilityRelation"/>
        <rdfs:label xml:lang="en">Non navigability</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#NonlinearElement -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#NonlinearElement">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/topology/topo#NetElement"/>
        <rdfs:label xml:lang="en">Nonlinear element</rdfs:label>
        <skos:definition xml:lang="en">Net element with any number of ports except two. Used for expressing connexities and navigabilities outside a complex network area (e.g. a yard or a station).</skos:definition>
    </owl:Class>
    


    <!-- https://cdm.ovh/rsm/topology/topo#Port -->

    <owl:Class rdf:about="https://cdm.ovh/rsm/topology/topo#Port">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rsm/adapters/geosparql_adapter/geosparql_adapter#Feature"/>
        <rdfs:label>Port</rdfs:label>
        <skos:definition xml:lang="en">Any place, at the boundary of a net element, that can be an entry and/or exit point for rolling stock. In the case of linear elements, there are exactly two ports. These may coincide (loop case) or be distinct (general case).</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/list#OWLList -->

    <owl:Class rdf:about="https://w3id.org/list#OWLList"/>
</rdf:RDF>



