    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/abstract"/>
    


    <!-- http://purl.org/dc/elements/1.1/contributor -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/elements/1.1/type -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/type"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- https://vocab.org/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="https://vocab.org/vann/preferredNamespacePrefix"/>
    


    <!-- https://vocab.org/vann/preferredNamespaceURI -->


    <owl:AnnotationProperty rdf:about="https://vocab.org/vann/preferredNamespaceURI"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#float -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#float"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://qudt.org/schema/qudt/hasQuantityValue -->


    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/hasQuantityValue"/>
    


    <!-- http://qudt.org/schema/qudt/quantityKind -->


    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/quantityKind"/>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#applicationDirection -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#applicationDirection">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topology#LinearElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#Side"/>
        <rdfs:label xml:lang="en">application direction</rdfs:label>
        <skos:definition xml:lang="en">Allows to express the direction of travel for which the localized entity is relevant. Example: a signal generally applies to one travel direction; track occupancy detection systems may be oriented, etc.
Direction of travel is expressed relative to the orientation of the linear element, from port 0 to port 1, or intrinsic coordinate 0 to 1. It is NOT expressed with respect to &quot;normal travel direction in operations&quot;.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#associatedLinearElement -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#associatedLinearElement">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/localisation/localisation#LinearLocation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#ListedLinearElement"/>
        <rdfs:label xml:lang="en">associated linear element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#associatedNetElement -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#associatedNetElement">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/localisation/localisation#AreaLocation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#ListedNetElement"/>
        <rdfs:label xml:lang="en">net element list</rdfs:label>
        <skos:definition xml:lang="en">Property pointing to all net elements (or parts thereof) that describe the area location.
Note: the user may target ListedNetElement individuals (a subclass of BoundedNetElement) and define an order, for whatever purpose.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#elementLocation -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#elementLocation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/localisation/localisation#LocatedNetElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#BaseLocation"/>
        <rdfs:comment xml:lang="en">Provides the location(s) of a located net element. Locations may be representation- and time-dependent, so the property is not a functional property (in the sense of RDF/OWL).</rdfs:comment>
        <rdfs:label xml:lang="en">element Location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#lateralOffset -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#lateralOffset">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/localisation/localisation#offset"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">lateral offset</rdfs:label>
        <skos:definition xml:lang="en">offset of location (usually: spot location) with respect to a perpendicular to the linear element, in the horizontal plane (not: track plane). Is positive when on the left and negative on the right (in order to respect the conventional orientation of cartesian reference systems, where X is the longitudinal axis, Y the transversal, and Z the vertical).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#offset -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#offset">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/schema/qudt/hasQuantityValue"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topology#LinearElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#OffsetValue"/>
        <rdfs:comment xml:lang="en">definitely not functional, to cope with the fact that values may be multiple (specified, observed...).
definitely inverse functional, because values characterize a location relative to exactly one linear element. &quot;Exactly&quot; however is left for SHACL shapes (not in RL profile).</rdfs:comment>
        <rdfs:label xml:lang="en">offset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#onLinearElement -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#onLinearElement">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/localisation/localisation#SpotLocation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#BoundedLinearElement"/>
        <rdfs:comment xml:lang="en">provides the linear element(s) that define a spot location. There can be several linear elements involved: think of a signal adderessing two adjacent tracks.</rdfs:comment>
        <rdfs:label>on linear element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#side -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#side">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topology#LinearElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/localisation/localisation#Side"/>
        <rdfs:comment xml:lang="en">consistency with lateral offset sign to be verified by SHACL shapes.</rdfs:comment>
        <rdfs:label xml:lang="en">side</rdfs:label>
        <skos:definition xml:lang="en">side of the linear element on which the location (usually a spot location) is situated.
The orientation of the linear element is its own intrinsic orientation, from port 0 to port 1 (or intrinsic coordinate 0 to intrinsic coordinate 1).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#tangentialOffset -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#tangentialOffset">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/localisation/localisation#offset"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">tangential offset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/localisation/localisation#verticalOffset -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/localisation/localisation#verticalOffset">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/localisation/localisation#offset"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">vertical offset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#connectedWith -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#connectedWith">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#connectedWithTransitive"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:label xml:lang="en">connected with</rdfs:label>
        <skos:definition xml:lang="en">Relation between a port on a net element and the asjacent port on another net element. Geographic locations of related ports may not coincide, either because of uncertainties or rounding errors, or by construction (e.g. case of tracks joined via a turntable).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#connectedWithTransitive -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#connectedWithTransitive">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">connected with transitive</rdfs:label>
        <skos:definition xml:lang="en">transitive closure of connectedWith.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#connectsPort -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#connectsPort">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#hasConnection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#Connection"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:comment xml:lang="en">This object property points to (at least two) ports that share a connection relationship.

It is used for building the connectedWith properties. It can be a temporal object, although this is not (yet) the case in this version.</rdfs:comment>
        <rdfs:label xml:lang="en">connects port</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#fromExitPort -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#fromExitPort">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#navigabilityRelationFrom"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#NavigabilityRelation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">from exit port</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#hasConnection -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#hasConnection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">inverse property of &apos;connects port&apos;. Instrumental in building the property chain resulting in &apos;connected with&apos; (a relation between ports)</rdfs:comment>
        <rdfs:label xml:lang="en">has connection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#leadsTo -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#leadsTo">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#leadsToTransitive"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#port"/>
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#navigableTo"/>
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#onElement"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">leads to</rdfs:label>
        <skos:definition xml:lang="en">relation between two adjacent net elements (i.e. having connected ports), where rolling stock can start moving from the first and arrive on the second without reversing travel direction. This property is not symmetric.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#leadsToTransitive -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#leadsToTransitive">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#NetElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#NetElement"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#port"/>
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#navigableToTransitive"/>
            <rdf:Description rdf:about="https://cdm.ovh/rsm/topology/topo#onElement"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">leads to transitive</rdfs:label>
        <skos:definition xml:lang="en">relation between net elements (adjacent or not), where rolling stock may leave the first and reach the second without reversing travel direction.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigabilityRelationFrom -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigabilityRelationFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">navigability relation from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigabilityRelationTo -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigabilityRelationTo">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#toExitPort"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">navigability relation to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigableFrom -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigableFrom">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#navigableFromTransitive"/>
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#navigableTo"/>
        <rdfs:label xml:lang="en">navigable from</rdfs:label>
        <skos:definition xml:lang="en">inverse of navigable to</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigableFromTransitive -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigableFromTransitive">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#navigableToTransitive"/>
        <rdfs:label xml:lang="en">navigable from transitive</rdfs:label>
        <skos:definition xml:lang="en">inverse of navigable to transitive</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigableTo -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigableTo">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#navigableToTransitive"/>
        <rdfs:label xml:lang="en">navigable to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#navigableToTransitive -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#navigableToTransitive">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">navigable to transitive</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#nonNavigableFrom -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#nonNavigableFrom">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#nonNavigableTo"/>
        <rdfs:label xml:lang="en">non navigable from</rdfs:label>
        <skos:definition xml:lang="en">inverse of non navigable to</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#nonNavigableTo -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#nonNavigableTo">
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">non navigable to</rdfs:label>
        <skos:definition xml:lang="en">Expresses that there is no navigability link between the related ports. Given the OWA (open world assumption), a navigability not expressed does not mean &quot;non navigability&quot;. Consequently, non-navigability should be explicitly stated, as it is important knowledge.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#onElement -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#onElement">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rsm/topology/topo#port"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">on element</rdfs:label>
        <skos:definition xml:lang="en">inverse of port, port0, port1</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#port -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#port">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rsm/topology/topo#NetElement"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rsm/topology/topo#Port"/>
        <rdfs:label xml:lang="en">port</rdfs:label>
        <skos:definition xml:lang="en">Associated ports with net elements. This is an inverse functional property, meaning that a port can belong to at most one net element (if a port is found to belong to two net elements, the inference is that those two elements are the same).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#port_0 -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#port_0">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#port"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <owl:propertyDisjointWith rdf:resource="https://cdm.ovh/rsm/topology/topo#port_1"/>
        <rdfs:label xml:lang="en">port 0</rdfs:label>
        <skos:definition xml:lang="en">First (or start) port of some net element. Allows to orient linear elements. Can also be used on others, with undefined semantics.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rsm/topology/topo#port_1 -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rsm/topology/topo#port_1">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rsm/topology/topo#port"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
